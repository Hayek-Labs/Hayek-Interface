{
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npx tailwindcss -i ./src/tailwind-input.css -o ./src/tailwind.css --watch\" \"umi dev\"",
    "build": "npx tailwindcss -i ./src/tailwind-input.css -o ./src/tailwind.css && umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@types/chroma-js": "^2.1.3",
    "@web3-react/core": "8.0.23-beta.0",
    "@web3-react/metamask": "8.0.19-beta.0",
    "@web3-react/types": "8.0.12-beta.0",
    "bignumber.js": "^9.0.2",
    "chroma-js": "^2.4.2",
    "clsx": "^1.1.1",
    "ethers": "^5.5.4",
    "lightweight-charts": "^3.8.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-icons": "^4.3.1",
    "react-select": "^5.2.2",
    "react-spring": "^9.4.3",
    "recharts": "^2.1.9",
    "three": "^0.137.5",
    "tw-elements": "^1.0.0-alpha12",
    "umi": "^3.5.20",
    "web3": "^1.7.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2",
    "umi-plugin-tailwindcss": "^3.2.1",
    "yorkie": "^2.0.0"
  }
}
